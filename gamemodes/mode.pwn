stock SendClientMessageEx(playerid, color, const string[]) //
{
    new scm1[300], scm2[300], scm3[300];
    if(strlen(string) > 90)
    {
        strmid(scm1, string, 0, 90);
        strmid(scm2, string, 90, strlen(string));
    }
    if(strlen(string) > 90)
    {
        format(scm3, 180, "%s", scm1);
        SendClientMessage(playerid, color, scm3);
        format(scm3, 180, "%s", scm2);
        SendClientMessage(playerid, color, scm3);
    }
    else SendClientMessage(playerid, color, string);
    return 0;
}
