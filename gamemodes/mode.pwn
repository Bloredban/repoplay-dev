CMD:unloadtruck(playerid, params[])
{
	if(PlayerLogged[playerid] == 0) return true;
	if(PlayerInfo[playerid][pJob] != J_TRUCKER) return SendClientMessage(playerid, COLOR_GREY, "Вы не развозчик продуктов!");
	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, COLOR_GREY, "Вы должны находиться за рулем дальнобоя!");
	if(PlayerToPoint(100.0, playerid, 3922.0066,-339.5015,3.9072))
	{
		if(GetPVarInt(playerid,"TRUCKVEH") != 0)
		{
			if(IsTrailerAttachedToVehicle(GetPlayerVehicleID(playerid)) != 1)
			{
				SendClientMessage(playerid, COLOR_GREY, "[ ! ] Вы потеряли свой груз!");
				ResetTruck(playerid);
				return true;
			}
			switch(GetPVarInt(playerid,"TruckType"))
			{
			case 1:
				{
					SendMes(playerid, -1, "{5CACEE}[ & ]  {FFFFFF}Вы разгрузили %d кг угля и получили %d руб.!",GetPVarInt(playerid,"TruckGruz"),GetPVarInt(playerid,"TruckGruz")*15);
					GiveMoney(playerid,GetPVarInt(playerid,"TruckGruz")*15);
					PlayerInfo[playerid][pTruckSkill] += GetPVarInt(playerid,"TruckGruz")/3;
					ResetTruck(playerid);
				}
			case 2:
				{
					SendMes(playerid, -1, "{5CACEE}[ & ]  {FFFFFF}Вы разгрузили %d шт запчастей и получили %d руб.!",GetPVarInt(playerid,"TruckGruz"),GetPVarInt(playerid,"TruckGruz")*25);
					GiveMoney(playerid,GetPVarInt(playerid,"TruckGruz")*25);
					PlayerInfo[playerid][pTruckSkill] += GetPVarInt(playerid,"TruckGruz")/3;
					ResetTruck(playerid);
				}
			}
		}
	}
	else
	{
		SendClientMessage(playerid, COLOR_GREY, "[ ! ] Вы должны находиться около порта");
	}
	return true;
}


