		format(str, 128, "{EE9A00}[ %s : %s ] {EE5555}%s: ",FracInfo[PlayerInfo[playerid][pMember]][fName],FracRank[PlayerInfo[playerid][pMember]][PlayerInfo[playerid][pRank]],PlayerInfo[playerid][pSendername]);
		SendClientMessageToAll(COLOR_GREEN,str);
		format(totalstring, 128, "%s", params[0]);
		SendClientMessageToAll(COLOR_GREEN,totalstring);
		SendClientMessageToAll(COLOR_WHITE, "");
		printf("%s %s: %s",FracRank[PlayerInfo[playerid][pMember]][PlayerInfo[playerid][pRank]],PlayerInfo[playerid][pSendername], params[0]);
	}
	else SendClientMessage(playerid, COLOR_GREY, YouCanNot);
