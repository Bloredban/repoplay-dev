public OnVehicleSpawn(vehicleid)
{
	if(Mcar[vehicleid] > 0) Mcar[vehicleid] = 0;
    DestroyObject(Blink[vehicleid][0]);
	DestroyObject(Blink[vehicleid][1]);
	VehInfos[vehicleid][vRightBlink] = 0;
	DestroyObject(Blink[vehicleid][2]);
	DestroyObject(Blink[vehicleid][3]);
	VehInfos[vehicleid][vLeftBlink] = 0;
	SetVehicleNumberPlate(vehicleid,"Ortiplay");
	if(vehicleid >= trucker1lvl[0] && vehicleid <= trucker1lvl[1] || vehicleid >= mechanic[0] && vehicleid <= mechanic[1] || vehicleid >= nouncar[0] && vehicleid <= nouncar[1] || vehicleid >= taxicar[0] && vehicleid <= taxicar[1] || vehicleid >= buscar[0] && vehicleid <= buscar[1])
	{
	    	Fuel[vehicleid] = 100;
	}
	if(MatsVehicle[vehicleid] > 0) MatsVehicle[vehicleid] = 0;
	if(rent_moped[vehicleid] > 0)
	{
        rent_Vehicle[rent_moped[vehicleid]-1] = 0;
     	rent_moped[vehicleid] = 0;
		Fuel[vehicleid] = 100;
		SetVehicleHealth(vehicleid, 1000);
	}
	if(Robbery[vehicleid] == true)
	{
		Robbery[vehicleid] = false;
		Boot[vehicleid] = false;
		Delete3DTextLabel(MatsText[vehicleid]);
		DestroyPickup(MatsPickup[vehicleid]);
	}
	if(vehiclerun[vehicleid] == 1)
	{
		Delete3DTextLabel(veh3dtext);
		Seeds[vehicleid] = 0;
		DestroyPickup(farmpickup);
		vehiclerun[vehicleid] = 0;
	}
	if(MatsStatus[vehicleid] == true)
	{
		MatsStatus[vehicleid] = false;
		Boot[vehicleid] = false;
		Delete3DTextLabel(MatsText[vehicleid]);
		DestroyPickup(MatsPickup[vehicleid]);
	}
	for(new v = 1; v <= 5; v++)
	{
		if(vehicleid == VehInfo[v][vVehicle])
		{
			if(GetVehicleModel(vehicleid) == VEHICLE_ATM)
			{
				for(new i = 0; i <= 3; i++)
				{
					if(vehicle_atm[vehicleid][i] == 0) break;
					if(i < 2) AttachObjectToVehicle(vehicle_atm[vehicleid][i], vehicleid, -0.5+i, -1.0, 0.5, 0.0, 0.0, 0.0);
					else AttachObjectToVehicle(vehicle_atm[vehicleid][i], vehicleid, -2.5+i, -2.0, 0.5, 0.0, 0.0, 0.0);
				}
			}
			LinkVehicleToInterior(VehInfo[v][vVehicle], VehInfo[v][vInt]);
			SetVehicleVirtualWorld(VehInfo[v][vVehicle], VehInfo[v][vWorld]);
		}
	}
	if(GetVehicleModel(vehicleid) == 462) Fuel[vehicleid] = 50;
	SetVehicleParamsEx(vehicleid,false,false, false,false,false,false,false);
	Engine[vehicleid] = false;
	Lights[vehicleid] = false;
	return true;
}
