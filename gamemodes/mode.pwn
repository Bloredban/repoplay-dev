CMD:dice(playerid, params[], help)
{
	new YCMDstr[255];
    if(PlayerLogged[playerid] == 0) return true;
    if(!PlayerToPoint(50.0,playerid,1680.9194, 2061.6453, -8.2148)||!PlayerToPoint(50.0,playerid,1685.1372, 2061.7505, -8.2148)||
	!PlayerToPoint(50.0,playerid,1685.1581, 2075.0271, -8.2148)||!PlayerToPoint(50.0,playerid,1681.0664, 2074.9504, -8.2148)) return SendClientMessage(playerid, -1, "Вы должны находиться в казино!");
    if(sscanf(params, "ud", params[0],params[1])) 	return SendClientMessage(playerid, COLOR_WHITE, "Введите: /dice [id игрока] [ставка]");
    if(params[1] < 100 || params[1] > 5000000) 		return SendClientMessage(playerid, -1, "Минимальная ставка 100 рублей максимальная 5.000.000 рублей");
    if(ZapretDice[params[0]] == 1) 					return SendClientMessage(playerid, -1, "В данный момент игрок не может играть!");
    if(PlayerInfo[playerid][pCash] < params[1]) 	return SendClientMessage(playerid, -1, "У вас нету столько денег!");
    if(PlayerInfo[params[0]][pCash] < params[1]) 	return SendClientMessage(playerid, -1, "У этого игрока нет столько денег!");
    if(!IsPlayerConnected(params[0])) return true;
	if(!ProxDetectorS(6.0, playerid, params[0])) return true;
    if(params[0] == playerid) return true;
    format(YCMDstr, sizeof(YCMDstr), "Вы предложили %s бросить кости. Ставка: рублей %i",GN(params[0]),params[1]);
    SendClientMessage(playerid, 0x6495EDFF, YCMDstr);
    format(YCMDstr, sizeof(YCMDstr), "%s предлагает бросить кости. Ставка: рублей %i",GN(playerid),params[1]);
    SendClientMessage(params[0], 0x6495EDFF, YCMDstr);
    SendMes(params[0], COLOR_WHITE, "Нажмите {48FC23}Y {FFFFFF}чтобы согласиться или {FF0000}N {FFFFFF}для отказа.");
    ZapretDice[playerid] = 1;
    ZapretDice[params[0]] = 1;
    KostiName[params[0]] = playerid;
    KostiMoney[params[0]] = params[1];
    SetPVarInt(params[0],"dice",1);
    return true;
}
