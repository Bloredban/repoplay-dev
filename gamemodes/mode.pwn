stock IsAIP(text[])
{
	new numbers;
	for(new i = 0;i < strlen(text); i++)
	{
	    if('0' <= text[i] <= '9')
	    {
	        if(!('0' <= text[i+1] <= '9'))
	        {
	            numbers ++;
	        }
	    }
	}
	if(numbers >= 4) return true;
	return false;
}
stock IsASymbol(text[], symbol, first, end)
{
	if(first < 0) first = 0;
	if(end >= strlen(text)) end = strlen(text) - 1;
	for(new i = first; i <= end; i ++)
	{
	    if(text[i] == symbol)
		{
			return true;
		}
	}
	return false;
}
