stock IsAGoodName(Name[])
{
	new NameChars;
	if(strlen(Name) < 2) return false;
	if(Name[0] < 'A' || Name[0] > 'Z') return false;
	for(new i = 1; i < strlen(Name); i++)
	{
	    if(Name[i] < 'a' || Name[i] > 'z')
		{
		    if(Name[i] >= 'A' && Name[i] <= 'Z') NameChars ++;
		    else return false;
		}
	    if(NameChars > 0) return false;
	}
	return true;
}

