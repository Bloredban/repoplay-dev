		GetVehiclePos(vehid,vehz,vehz,vehz);
		if(vehz > 1300.000)
		{
			SetVehicleToRespawn(vehid);
			KickEx(playerid,119);
		}
		if(GetPVarInt(playerid,"AC_SECSD") == second)
