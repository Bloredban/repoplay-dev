CMD:rub(playerid, params[])
{
	if(IsPlayerConnected(playerid) && PlayerInfo[playerid][pAdminn] >= 1)
	{
		DeleteClosestRoadblock(playerid);
	}
	return 1;
}
CMD:rrball(playerid, params[])
{
    if(IsPlayerConnected(playerid) && PlayerInfo[playerid][pAdminn] >= 1)
    {
        DeleteAllRoadblocks(playerid);
    }
	return 1;
}

CMD:givevb(playerid, params[])
{
	if(PlayerLogged[playerid] == 0) return true;
    if(PlayerInfo[playerid][pMember] > 10 || PlayerInfo[playerid][pLeader] > 10) return SendClientMessage(playerid, COLOR_GREY, "[ ! ] У вас нет возможности выдавать военный билет");
	if(sscanf(params, "u", params[0])) return SendClientMessage(playerid, COLOR_GREY, "• {ffcf00}[ Подсказка ] {ffffff}Введите: /givevb [id игрока]");
	if(PlayerInfo[params[0]][pMember] != 10) return SendClientMessage(playerid, COLOR_GREY, "[ ! ] Данный персонаж не из военнослужащий");
	if(PlayerInfo[params[0]][pMember] > 3)
	{
	    PlayerInfo[params[0]][pVb] = 1;
	    SendClientMessage(params[0], COLOR_GREEN, "Вам выдали военный билет");
		SendClientMessage(playerid, COLOR_GREEN, "Вы выдали военный билет военнослужащему");
	}
	else SendClientMessage(playerid, COLOR_GREY, "[ ! ] Выдавать можно выше 3 ранга");
	return true;
}
CMD:agivevb(playerid, params[])
{
	if(PlayerLogged[playerid] == 0) return true;
	if(PlayerInfo[playerid][pAdminn] == 1) return true;
	if(sscanf(params, "u", params[0])) return SendClientMessage(playerid, COLOR_GREY, "• {ffcf00}[ Подсказка ] {ffffff}Введите: /agivevb [id игрока]");
	{
	    PlayerInfo[params[0]][pVb] = 1;
	    SendClientMessage(params[0], COLOR_GREEN, "Администратор выдал вам военный билет");
		SendClientMessage(playerid, COLOR_GREEN, "[ A ] Вы выдали военный билет игроку");
	}
	return true;
}
CMD:getpick(playerid, params[])
{
	if(PlayerLogged[playerid] == 0) return true;
	if(PlayerInfo[playerid][pAdminn] < 1 || AdminLogged[playerid] == false) return true;
	if(Pick == false) return SendClientMessage(playerid, COLOR_GREY, "В области нет выборов!");
	GetPick(playerid);
	return true;
}

CMD:pick(playerid, params[])
{
	if(PlayerInfo[playerid][pAdminn] < 4 || AdminLogged[playerid] == false) return true;
	ShowPlayerDialog(playerid, D_PICK+3, 2, "Выборы", "Начать выборы\nЗакончить выоборы\nИзменить имена кандидатов", "Принять", "Отмена");
	return true;
}

CMD:picklist(playerid, params[])
{
	if(PlayerLogged[playerid] == 0) return true;
	if(Pick == true) return SendClientMessage(playerid, COLOR_GREY, "В области не закончены выборы!");
	GetPick(playerid);
	return true;
}
CMD:setkazna(playerid, params[])
{
	if(PlayerLogged[playerid] == 0) return true;
	if(sscanf(params, "i", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "• {ffcf00}[ Подсказка ] {ffffff}Введите: /setkazna [количество]");
	if(GetMoney(playerid) < params[0]) return true;
	if(PlayerInfo[playerid][pAdminn] > 4 || PlayerInfo[playerid][pLeader] == F_MAYOR || PlayerInfo[playerid][pMember] == F_MAYOR && PlayerInfo[playerid][pRank] == 11)
	{
		Exchequer+=params[0];
		GiveMoney(playerid, -params[0]);
	}
	return true;
}
CMD:getkazna(playerid, params[])
{
	if(PlayerLogged[playerid] == 0) return true;
	if(sscanf(params, "i", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "• {ffcf00}[ Подсказка ] {ffffff}Введите: /getkazna [количество]");
	if(Exchequer < params[0]) return true;
	if(PlayerInfo[playerid][pAdminn] > 4 || PlayerInfo[playerid][pLeader] == F_MAYOR || PlayerInfo[playerid][pMember] == F_MAYOR && PlayerInfo[playerid][pRank] == 11)
	{
		Exchequer-=params[0];
		GiveMoney(playerid, params[0]);
	}
	return true;
}
