	format(totalstring,sizeof(totalstring),"INSERT INTO "TABLE_VEH" (model, x, y, z, fa, vint, world, colors, fraction, status) VALUES (%i, '%f', '%f', '%f', '%f', %i, %i, '%s', %i, %i)",
