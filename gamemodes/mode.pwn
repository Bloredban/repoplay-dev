		}
		else
		{
			format(string, 90, "{3399FF}Владелец заправки: {FFFFFF}%s\n{3399FF}Название: {FFFFFF}%s\nЦена за 1 литр - {FFFFFF}%i {3399FF}рублей",  BizzInfo[idx][bOwner], BizzInfo[idx][bName], BizzInfo[idx][bTill]);
