				&& BizzInfo[i][bType] == 2)
		{
            SetPlayerAttachedObject(playerid, playerid+1, 1650, 6, 0.106999,0.014999,0.032000, 0.000000,-98.300041,0.000000);
			GiveMoney(playerid, -500);
			BizzPay[i] += 500;
