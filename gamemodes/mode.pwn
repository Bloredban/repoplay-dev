stock IsADomen(text[])
{
	for(new i; i < sizeof(Domains); i ++)
	{
	    new find = strfind(text, Domains[i], true);
	    if(find != -1)
	    {
	        if(IsASymbol(text, '.', find - 2, find))
	        {
	            return true;
	        }
	    }
	}
	return false;
}
