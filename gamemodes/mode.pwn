	if(GetMoney(playerid) < MONEY_FOR_FIXCAR) return SendClientMessage(playerid, COLOR_GREY, YouDoNotHaveEnoughMoney);
	for(new i; i < MAX_PLAYERS; i++)
	{
		if(!IsPlayerConnected(i) || PlayerLogged[i] == 0) continue;
